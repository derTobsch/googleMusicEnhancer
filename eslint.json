{
    "env": {
        "node": true,
        "browser": true,
        "jquery": true
    },
    "rules": {
        "quotes": [2, "single", "avoid-escape"],
        "no-use-before-define": [2, "nofunc"],
        "new-cap": [0],
        "no-unused-vars": [0],
        "no-undef": [0],
        "camelcase": [0]

    },
    "globals": {
        "GM_setValue": true,
        "GM_getValue": true,
        "GM_deleteValue": true,
        "GM_xmlhttpRequest": true,
        "GM_addStyle": true,
        "GM_info": true
    }
}